datos = read.csv("analisisTarea6.dat", sep=" ", header=FALSE)
a = aggregate(datos, by=list(datos$V1), mean)
plot(a$Group.1, a$V2, col="blue", pch=16, type="l")
lines(a$Group.1, a$V3, col="red", pch=16)
lines(a$Group.1, (a$Group.1* log(a$Group.1)), col="yellow", pch=16)
lines(a$Group.1, (a$Group.1* a$Group.1), col="green", pch=16)
