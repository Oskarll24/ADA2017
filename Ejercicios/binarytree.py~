class node:
    def __init__(self,val):
        self.value = val
        self.count =1
        self.left = None
        self.right = None
    def add(self,new):
        if new  < self.value:
            #ver si hay nodo hijo
            if self.left is None:
                self.left = node(new)
            else:
                self.left.add(new)
        elif new > self.value:
            if self.right is None:
                self.right = node(new)
            else:
                self.right.add(new)
        else:
            self.count += 1
            
    def search(self,i):
        if self.value == i:
            return self
        elif self.value > i:
            if self.left is None:
                return False
            return self.left.search(i)
        else:
            if self.right is None:
                return False
            return self.right.search(i)
                
            
raiz = node(13)
raiz.add(16)
raiz.add(23)
raiz.add(4)
print(raiz.search(8))
print(raiz.search(23).count)
raiz.add(23)
print(raiz.search(23).count)
    
raiz.add(16)
