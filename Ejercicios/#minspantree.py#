def minspantree(G):    
    d = dict()
    with open(G, 'r') as lol:
        n = int(lol[0])
        for l in lol[1:]:
            e = (l.strip()).split()
            x, y, w = e.pop(), e.pop(), e.pop()
            
            u = d.get(x, [{x},w])
            v = d.get(y, [{y},w])

            r = [u[0]|v[0], ]
            
            if a in r and b in r:
                return True
            
            for v in r[0]:
                d[v] = r
            print(d)
    return False
    
